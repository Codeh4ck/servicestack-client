{
  "name": "servicestack-client",
  "title": "ServiceStack JavaScript Utils",
  "version": "0.0.3",
  "description": "ServiceStack's TypeScript library providing a number of convenience utilities in developing web apps. Integrates with ServiceStack's Server features including Error Handling, Validation and Server Events",
  "homepage": "https://github.com/ServiceStack/servicestack-client",
  "bugs": "https://github.com/ServiceStack/Issues",
  "author": {
    "name": "Service Stack",
    "email": "team@servicestack.net",
    "url": "https:/servicestack.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServiceStack/servicestack-client.git"
  },
  "scripts": {
    "pretest": "node_modules/.bin/tsc",
    "test": "mocha -t 5000 ./tests/*.js",
    "prepublish": "node_modules/.bin/tsc && node scripts/prepack"
  },
  "files": [
    "lib/src/**/*"
  ],
  "keywords": [
    "servicestack",
    "utils",
    "validation",
    "ajax",
    "serverevents",
    "error-handling"
  ],
  "licenses": [],
  "main": "lib/index.js",
  "typings": "lib/index",
  "dependencies": {
    "es6-shim": ">=0.35.0",
    "isomorphic-fetch": ">=2.2.0",
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.5.3"
  }
}
